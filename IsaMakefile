## targets

default: UTP
images: 
test: UTP

all: images test


## global settings

SRC = $(ISABELLE_HOME)/src
OUT = $(ISABELLE_OUTPUT)
LOG = $(OUT)/log

USEDIR = $(ISABELLE_TOOL) usedir -v true -i true -d pdf -b


## UTP

UTP: $(LOG)/UTP.gz

$(LOG)/UTP.gz: ## UTP/ROOT.ML UTP/document/root.tex UTP/*.thy
	@$(USEDIR) Datatype_Order_Generator UTP


## clean

clean:
	@rm -f $(LOG)/UTP.gz
